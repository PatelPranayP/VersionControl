FUNCTION "fcRescale" : Real
{ S7_Optimized_Access := 'TRUE' }
VERSION : 0.1
   VAR_INPUT 
      InMin : Real;
      InMax : Real;
      InVal : Real;
      OutMin : Real;
      OutMax : Real;
   END_VAR


BEGIN
	// Real-to-real rescaling of a value
	
	#fcRescale := #OutMax - ((#OutMax - #OutMin) / (#InMax - #InMin)) * (#InMax - #InVal);
END_FUNCTION

